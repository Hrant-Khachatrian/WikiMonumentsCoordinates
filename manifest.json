{
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "WikiMonumentsCoordinates.js" ],
      "matches": [ "http://hy.wikipedia.org/*", "https://hy.wikipedia.org/*" ]
   } ],
   "description": "Allows to easily add coordinates to the monuments inside Armenian Wikipedia using Google Maps",
   "name": "Add Monument Coordinates in Wikipedia",
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.1",
   "manifest_version": 2,
   "web_accessible_resources": [
	   "morebits.js",
	   "form.js"
   ]
}
